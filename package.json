    "clean": "if [ -d dist ]; then rm -r dist; fi",
    "cpsetup1": "cp package.json  dist/package.json",
    "cpsetup2": "cp .firebaserc   dist/.firebaserc",
    "cpsetup3": "cp 'firebase.json' dist/firebase.json",
